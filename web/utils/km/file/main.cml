#myGallery
{
    background-color: #000;
    overflow: auto;
    position: relative;
    font-family: Arial;
}

#myGallery, #myGallery input
{
    font: 11px Tahoma;
}

/*   Gallery Style
---------------------------------------*/
div.albumContainert
{
    text-align: center;
    float: left;
    margin: 10px 0px 5px 15px;
    cursor: pointer;
    background: url(/themes/images/share/album.gif) no-repeat left top;
    overflow: hidden;
    padding: 10px 20px 0 10px;
}

div.albumThumbnail
{
}

div.albumDescription
{
    padding-top: 14px;
    color: #fff;
}

div.albumDescription strong
{
    display: block;
    height: 15px;
    overflow: hidden;    
}

div.albumDescription span
{
    display: block;
    height: 10px;
    color: #aaa;
    font-size: 9px;
}

/*   Album Style
----------------------------------------*/
#thumbnailsContainer
{
    overflow: hidden;
    position: relative;
    background-color: #222;
    border-bottom: #999;
    margin: 0 12px;padding: 5px 0;
}

#thumbnailsHolder
{
    position: relative;
    left: 0;
    
}

#thumbnailsHolder div
{
    border: solid 1px #3B3A37;
    margin: 0 5px;
}

#thumbnailsHolder td.selected div
{
    border: solid 1px #fff;
}

#previewContainer
{
    text-align: center;
    background-color: #000;
}

#navContainer
{
    border: solid 1px #666;
    background-color: #222;
    padding-top: 5px;
    text-align: center;
    height: 25px;
}

#navContainer input
{
    border: solid 1px #666;
    background-color: #444;
    color: #fff;
    height: 20px;
    margin: 0 2px;
    cursor: pointer;
}

#navContainer input:hover
{
    background-color: #555;
}

#navContainer input.scrollLeft, #navContainer input.scrollRight
{
    position: absolute;
    top: 0;
    width: 10px;
    height: 70px;
    overflow: hidden;
    padding: 0;    
    margin: 1px;
}

input.scrollLeft
{
    left: 0;
}

input.scrollRight
{
    right: 0;
}

/* Slide Show Style
-------------------------------------------*/
div.ssMask
{
    background-color: #000;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 12002;
}

div.ssNav
{
    background-color: #0A1621;
    border: solid 1px #464646;
    border-bottom-width: 0;
    width: 390px;
    margin: 0 auto;
    padding: 10px;
    color: #fff;
}

div.ssNav, div.ssNav input
{
    font: 11px Tahoma;
}

div.ssPreview
{
    padding: 15px 20px 5px 20px;
    text-align: center;
    overflow: hidden;
}

div.ssComment
{
    height: 10px;
    padding: 2px 10px 10px 10px;
    text-align: center;
    overflow: hidden;
    color: #aaa;
    margin: 5px auto;
}

div.ssNav a
{
    color: #fff;
    text-decoration: underline;
}

div.ssNav a:hover
{
    text-decoration: none;
}

div.ssNav input
{
    margin: 0 3px;
    padding: 0;
    color: #fcfcfc;
    border: solid 1px #647687;
    background-color: #1C3D59;
    font: bold 11px Arial;
    cursor: pointer;
}

div.ssNav input:hover
{
    background-color: #255277;
}

div.ssNav input.ply
{
    width: 50px;
}

div.ssNav input.inc
{
    margin-right: 15px;
}

div.ssNav input.dec
{
    margin-left: 15px;
}

div.ssNav input.cls
{
    margin-left: 17px;
}

