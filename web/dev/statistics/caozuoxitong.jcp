	public void doPost(Jacper jacper) {
	try {
			HttpSession hs = jacper.getSession();
			Connection conEdu = jacper.getConnection("education");
			String startdate = jacper.getString("startdate");
			String enddate=jacper.getString("enddate");
			enddate+="  23:59:59";
			JSONObject jbout=new JSONObject();
			JSONArray ja = new JSONArray();
			String sql="select computer_system,count(*),count(distinct computer_mac),count(distinct ip) from tongjiguanli where computer_system is not null and  login_time>=str_to_date('"+startdate+"','%Y/%m/%d') and login_time<=str_to_date('"+enddate+"','%Y/%m/%d %T') group by computer_system";
			
			String[][]rs=conEdu.get(sql);
			double count=0;
            for(int i=0;i<rs.length;i++){
                count+=Integer.parseInt(rs[i][1]);
            }
			
			
			for(int i=0;i<rs.length;i++){
				JSONObject jb=new JSONObject();
				jb.put("xitong",rs[i][0]);
				jb.put("pv",rs[i][1]);
				jb.put("uv",rs[i][2]);
				jb.put("ip",rs[i][3]);
				
				double baifen=Math.round(Integer.parseInt(rs[i][1])/count*100*100)/100.0;
				jb.put("pvbili",baifen+"%");
				ja.put(jb);
			}
			jbout.put("authArray",ja);
			jacper.print(jbout);
	} catch (Exception e) {
			e.printStackTrace();
	}
	}}