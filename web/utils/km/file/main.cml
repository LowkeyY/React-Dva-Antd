<root><class name="utils.km.file.main"><main><![CDATA[/* Change Tree Collapse and Expand Icons */
.db-folders-tree .x-tree-elbow-plus,
.db-folders-tree .x-tree-elbow-end-plus {
    background-image: url( /themes/images/desktop/tree_expand.gif ) !important;
}
.db-folders-tree .x-tree-elbow-minus,
.db-folders-tree .x-tree-elbow-end-minus { 
    background-image: url( /themes/images/desktop/tree_collapse.gif ) !important;
}

/* Upload Dialog Style */
.x-dlg-upload
{
}

.x-dlg-upload div.hlp
{
    padding: 0 0 10px 20px;
    height: 16px;
    background: url(/themes/icons/xp/info.gif) no-repeat left top;
}
.x-dlg-upload div.swc
{
    padding: 0 0 5px 20px;
    height: 16px;
    background: url(/themes/icos/xp/switcher.png) no-repeat left 0;
}

.x-dlg-upload div.prg
{
    padding-top: 5px;
}

.x-dlg-upload div.prg span
{
    color: #aaa;
    font: bold 12px Arial;
}

.x-dlg-upload div.prg table
{
    margin-top: 15px;    
}

.x-dlg-upload div.prg td
{
    padding: 4px 0;
}

.x-dlg-upload-progress
{
    margin-bottom: 5px;
    height: 24px;
    border: 1px solid gray;
    background-color: #fff;
}

.x-dlg-upload-progress-bar
{
    height: 100%;
    background-color: Red;
    background: url(/themes/images/desktop/progress.gif) repeat-x;
}
.x-dlg-upload-status
{
    text-align: center;
    font-weight: bold;
    font-size: 1.2em;
}

.x-dlg-upload div.suc
{
    padding: 5px 0 3px 23px;
    overflow: hidden;
    background: url(/themes/icon/xp/check.gif) no-repeat 2px 5px;
}

/* property panel */
.db-property-panel .x-panel-body
{
    border-top: solid 1px #d0d0d0;
    overflow: hidden;
    background: #eee url(/themes/images/desktop/property.png) repeat-x top;
}
.db-property-panel td
{
    padding: 3px 4px;
	font-size: 0.8 em;
}
.db-property-panel td.ppLabel
{
    text-align: right;
    color: #aaa;
	font-size: 8px;
    font-weight: bold;
}
.db-property-panel td.ppTitle
{
    padding-left: 7px;
    color: #215DC6;
    font-weight: bold;
	font-size: 0.8 em;
}

/* progress bar */
.db-progress-bar
{
    padding: 5px;
}
.db-progress-bar p
{
    margin: 5px 0 5px 0;
    text-align: center;
    padding: 0;
}
.db-progress-bar div
{
    display: block;
    background: url(/themes/images/share/progress-bar.gif) no-repeat;
    height: 20px;
}
.db-progress-bar div span
{
    display: none;
}

/* Toolbars */
.db-menubar
{
    padding: 0px 3px;
}
.db-addressbar
{
    padding: 1px 0 0 3px;
}

.db-toolbar
{
    padding: 5px 3px;
}



.x-form-field-wrap .x-form-address-trigger {
    background-image: url(/themes/images/desktop/address-trigger.gif);
}


/* Files Grid Related Style */
.db-ellipsis
{
	overflow:hidden;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
    white-space: nowrap;      
}

.file-grid .x-grid3-row {
    border: none;
}

.db-fg-item 
{
    float: left;  
    width:90px !important;   
    height:90px !important;   
    margin: 5px 5px 10px 5px;  

}

/* Icons View */
.db-fg-iconView
{
    width: 62px;
    height: 52px;
    padding: 4px;
    overflow: hidden;
}
.db-fg-iconView .db-icn
{    
    height: 32px;
    background-repeat: no-repeat;
    background-position: center center;
}
.db-fg-iconView .db-txt
{
    text-align:center;
    height: 15px;
    margin-top: 5px;
}

.file-grid .x-grid3-row-over .db-fg-iconView
{
    background: #fff url(/themes/images/desktop/vw-icn.png) no-repeat left top;
}

.file-grid .x-grid3-row-selected .db-fg-iconView
{
    background: #fff url(/themes/images/desktop/vw-icn.png) no-repeat left bottom;
}

/* Details View */
.db-fg-detailView 
{
}

.db-fg-detailView .db-icn
{
    float: left;
    height: 16px;
    width: 16px;
}

.db-fg-detailView .db-txt 
{
    padding: 1px 0 0 2px;
}

.file-grid .x-grid3-cell-inner
{
    padding: 2px 4px 3px 4px;
}

/* Thumbnail View */
.db-fg-thumbView
{
    width: 82px;
    height: 82px;
    margin: 4px;
    overflow: hidden;
}
.db-fg-thumbView .db-icn
{
    height: 60px;
    border: solid 1px #E0E0E0;
    width: 80px;
    overflow: hidden;
    background-color: #fff;
    background-position: center center;
    background-repeat: no-repeat;
}
.db-fg-thumbView .db-icn table 
{
    width: 100%;
    height: 100%;
    background-color: #fff;
}
.db-fg-thumbView .db-icn table td
{
    vertical-align:middle;
    text-align: center;
}
.db-fg-thumbView .db-txt
{
    text-align:center;
    height: 18px;
    padding-top: 3px;
    margin-top: 3px;
}

.file-grid .x-grid3-row-over .db-fg-thumbView
{
    background: #fff url(/themes/images/desktop/vw-tmb.png) no-repeat left top;
}
.file-grid .x-grid3-row-selected .db-fg-thumbView
{
    background: #fff url(/themes/images/desktop/vw-tmb.png) no-repeat left bottom;
}
.file-grid .x-grid3-row-selected .db-fg-thumbView .db-icn
{
    border-color: #AACCF6;
}

/* Tiles View */
.db-fg-tileView
{
    width: 170px;
    height: 48px;
    padding: 4px;
    overflow: hidden;
}
.db-fg-tileView .db-icn
{    
    float: left;
    padding-right: 5px;
    width: 48px;
    height: 48px;
    background-repeat: no-repeat;
}
.db-fg-tileView .db-icn img
{
    width: 48px;
    height: 48px;
}
.db-fg-tileView .db-txt {
    padding-top: 3px;
}

.db-fg-tileView .db-txt span
{
    display: block;    
    color: #666;
    font-size: 0.9em;
    line-height: 14px;
}
.db-fg-tileView .db-txt span.nm
{
    color: #000;
    font-size: 1em;
}
.file-grid .x-grid3-row-over .db-fg-tileView
{
    background: #fff url(/themes/images/desktop/vw-til.png) no-repeat left top;
}

.file-grid .x-grid3-row-selected .db-fg-tileView
{
    background: #fff url(/themes/images/desktop/vw-til.png) no-repeat left bottom;
}

/* Upload From Url Style */
.db-ufu-win .x-panel-header
{
    background-image: none;
    border-bottom-style: none;
}
.db-ufu-win p.hlp
{
    padding: 8px 0 3px 25px;
    overflow: hidden;
    background-repeat: no-repeat;
    background-position:  7px 7px;
    font-weight: bold;
}
.db-ufu-win p.info
{
    padding: 10px 0 3px 30px;
    overflow: hidden;
    background: url(/themes/images/share/arrow-right.gif) no-repeat 20px 14px;
}
.db-ufu-win div.prg p.hlp
{
    background-image: url(/themes/icon/xp/info.gif);
}
.db-ufu-win div.prg p.bar
{
    display: block;
    background: url(/themes/images/share/progress-bar.gif) no-repeat center bottom;
    height: 20px;
}
.db-ufu-win div.prg p.bar span
{
    display: none;
}
.db-ufu-win div.prg p.center
{
    text-align: center;
}

.db-ufu-win div.suc p.hlp
{
    background-image: url(/themes/icon/xp/check.gif);
}
.db-ufu-win div.err p.hlp
{
    background-image: url(/themes/icon/xp/exclamation.gif);
}
