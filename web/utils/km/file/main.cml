.db-file-progress-wrap
{
    position: relative;
    border-bottom: solid 1px #e4e4e4;
    clear: both;
}
.db-file-progress-wbar
{
    height: 20px;
}
.db-file-progress-bar
{
    background: #6DE581 url(/themes/images/desktop/upload_progress.png) repeat-x top;
    visibility: hidden;
}
.db-file-progress-text
{
    height: 22px;
    position: absolute;
    top: 0;
    left: 0;
}
.db-file-progress-text div.name, .db-file-progress-text div.size, .db-file-progress-text div.stat
{
    display:table-cell;   
    float: left;
    line-height:20px;
    white-space:nowrap;
    overflow: hidden;
}
.db-file-progress-text div.name
{
    width: 314px;
    padding-left: 10px;
}
.db-file-progress-text div.size
{
    width: 70px;
    text-align: right;
    padding-right: 20px;
}
.db-file-progress-text div.stat
{
    width: 16px;
    padding-left: 10px;
    background-position: 10px 2px;
    background-repeat: no-repeat;
}

.db-file-progress-text div.stat span.cancel
{
    width: 16px;
    height: 16px;
    display: block;
    background: url( /themes/icon/xp/delete.png) no-repeat left top;
    cursor: pointer;
    margin-top: 2px;
}

.db-file-progress-cancel
{
    color: #aaa;
}
.db-file-progress-cancel span.cancel, 
.db-file-progress-complete span.cancel, 
.db-file-progress-error span.cancel
{
    visibility: hidden;
}
.db-file-progress-cancel div.stat
{
    background-image: url( /themes/icon/xp/bin.png)
}

.db-file-progress-complete
{
    color: #080;
}
.db-file-progress-complete div.stat
{
    background-image: url( /themes/icon/xp/check.gif)
}

.db-file-progress-error
{
    color: #dc143c;
}
.db-file-progress-error div.stat
{
    background-image: url( /themes/icon/xp/error.png)
}


/* Upload List Header Style */
.db-upload-files .x-panel-header
{
    background: #F9F9F9 url(/themes/images/desktop/upload_list_head.png) repeat-x top;
    height: 22px;
    border-bottom: solid 1px #D0D0D0;
    border-top: solid 1px #e6e6e6;
    padding: 1px 0 0 0;
    color: #666;
}
.db-upload-files .x-panel-header div.name, 
.db-upload-files .x-panel-header div.size, 
.db-upload-files .x-panel-header div.stat 
{
    display:table-cell;   
    float: left;
    line-height:22px;
    white-space:nowrap;
    overflow: hidden;
}
.db-upload-files .x-panel-header div.name
{
    border-right: solid 1px #D0D0D0;
    padding-left: 10px;
    width: 314px;
}
.db-upload-files .x-panel-header div.size
{
    border-right: solid 1px #D0D0D0;
    border-left: solid 1px #EEEEEE;
    padding-left: 20px;
    width: 68px;
}

.db-upload-progress-wrap
{
    background-color:#F8F8F8;
    padding:10px 5px 0 5px;
    border-bottom: solid 1px #e6e6e6;    
}

.db-upload-progress-table
{
    width: auto;
    border-collapse: collapse;
    margin-top: 7px;
}
.db-upload-progress-table .db-upload-progress-label
{
    width: 80px;
    text-align: left;
    padding: 2px 2px 2px 5px;
}
.db-upload-progress-table .db-upload-progress-value
{
    width:130px;
}

.db-upload-error-wrap
{
    background-color: #ffffe1;
    border-bottom: solid 1px #e4e4e4;
    clear: both;    
    height: 20px;
}
.db-upload-error-text div.name, .db-upload-error-text div.cancel
{
    display:table-cell;   
    float: left;
    line-height:20px;
    white-space:nowrap;
    overflow: hidden;
}
.db-upload-error-text div.name
{
    padding-left: 25px;    
    width: 395px;
    background: url( /themes/icons/xp/error.png) no-repeat 5px 2px;
}
.db-upload-error-text div.cancel
{
    padding-left: 8px;    
    width: 10px;
}
.db-upload-error-text div.cancel span.close
{
    width: 8px;
    height: 8px;
    display: block;
    background: url(/themes/images/desktop/close.gif) no-repeat left top;
    cursor: pointer;
    margin-top: 6px;    
}


.db-upload-header .x-panel-body
{
    padding: 10px 0 0 25px;
    color: #333;
    background: Transparent url( /themes/icon/xp/info.gif) no-repeat 5px 10px;
}
.db-upload-footer .x-panel-body
{
    padding: 5px 0 0 25px;
    color: #333;
    background: Transparent url( /themes/icon/xp/switcher.png) no-repeat 5px 5px;
}
.db-upload-footer .x-panel-body span 
{
    text-decoration: underline;
    cursor: pointer;
    color: #40608C;
}

.db-property-dialog .x-window-body {
    background-color: Transparent;
}

