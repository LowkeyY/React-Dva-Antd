				 sql="select map,count(*) from (select max(pagecount) map from tongjiguanli where shijian>=$str_date('"+startdate+"',111) and shijian<=$str_date('"+enddate+"',113)  group by huihuaid) group by map";
			
			String[][]rs=conEdu.get(sql);
